            "nextY": 5,
            "w": 6,
            "x": 0,
            "y": 0
          },
          "viz_type": "chart",
          "viz_content": "WITH obj AS\n  (\n    SELECT\n      _tp_time AS time, array_join(json_extract_array(raw, 'model_detection_prediction')) AS detected_objects, detected_objects:class_name AS name\n    FROM\n      video_stream_log\n    WHERE\n      _tp_time > (now() - 1h)\n  )\nSELECT\n  count(*) AS count, name, window_start\nFROM\n  hop(obj, time, 1s, 5s)\nGROUP BY\n  window_start, name\nORDER BY\n  count DESC",
          "viz_config": {
            "chartType": "bar",
            "config": {
              "color": "",
              "colors": [
                "#ED64A6",
                "#F0BE3E",
                "#DA4B36",
