              "dataLabel": true,
              "fractionDigits": 2,
              "gridlines": true,
              "groupType": "stack",
              "legend": false,
              "unit": {
                "position": "left",
                "value": ""
              },
              "updateKey": "window_start",
              "updateMode": "time",
              "xAxis": "name",
              "xFormat": "HH:mm",
              "xTickLabel": {
                "maxChar": 10
              },
              "xTitle": "",
