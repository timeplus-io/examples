        {
          "id": "0be6d2b0-a23c-4444-912c-809720bdd929",
          "title": "Violence Rate Detected in last 5 second",
          "description": "",
          "position": {
            "h": 4,
            "nextX": 6,
            "nextY": 9,
            "w": 6,
            "x": 0,
            "y": 5
          },
          "viz_type": "chart",
          "viz_content": "WITH vio AS\n  (\n    SELECT\n      _tp_time, raw:model_violence_predictions:top AS flag, if(flag = 'violence', cast(raw:model_violence_predictions:confidence, 'float64'), 0) AS vscore\n    FROM\n      video_stream_log\n    where _tp_time > now()-1h\n  )\nSELECT\n  window_start, count(*) AS count, sum(vscore) AS svscore, svscore / count AS violence_rate\nFROM\n  hop(vio, 1s, 5s)\nGROUP BY\n  window_start",
          "viz_config": {
            "chartType": "line",
            "config": {
              "color": "",
              "colors": [
